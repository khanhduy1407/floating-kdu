    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-kdu": "^8.0.3",
    "fs-extra": "^9.0.1",
    "kdu-template-compiler": "^2.6.14",
    "rollup": "^2.35.1",
    "rollup-plugin-analyzer": "^4.0.0",
